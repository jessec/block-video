{namespace site.block}
/**
 * @param url
 * @param content
 * @param title
 * @param position
 */
{template .video}
    <article class="core9-block video {$position}" data-type="video">
      <iframe width="100%" height="330px" src="{$url}" frameborder="0" allowfullscreen></iframe>
      <h1>{$title}</h1>
      <p>{$content|noAutoescape}</p>
    </article>
{/template}